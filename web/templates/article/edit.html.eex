<h1>Edit Article</h1>
<%= form_for @changeset, article_path(@conn, :update, @article), fn f -> %>
  <div class="form-group">
    <%= label f, :title %>
    <%= text_input f, :title %>
    <%= error_tag f, :title %>
  </div>
  <div class="form-group">
    <%= label f, :text %>
    <%= text_input f, :text %>
    <%= error_tag f, :text %>
  </div>
  <%= submit "Submit", class: "btn btn-primary" %>
<% end %>

<%= link "Back", to: article_path(@conn, :index) %>
